datasource db {
  provider = env("PRISMA_DB_PROVIDER")
  url      = env("PRISMA_DB_URL")
}

generator photon {
  provider = "photonjs"
}

model Container {
  id          String   @default(cuid()) @id
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  referenceID String   @unique
  slug        String
  user        User
}

model User {
  id         String      @default(cuid()) @id
  createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
  name       String      @unique
  containers Container[]
}